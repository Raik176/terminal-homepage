{
  "schemaVersion": 1,
  "id": "{{MOD_ID}}",
  "version": "${version}",
  "name": "{{MOD_NAME}}",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "license": "{{MOD_LICENSE}}",
  "icon": "assets/{{MOD_ID}}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      //<JAVA>
      "{{MOD_PACKAGE_NAME}}.ExampleMod"
      //</JAVA>
      //<KOTLIN>
      {
        "adapter": "kotlin",
        "value": "{{MOD_PACKAGE_NAME}}.ExampleMod"
      }
      //</KOTLIN>
    ]
  },
  "mixins": [
    "{{MOD_ID}}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "equilinox": "~1.7.2",
    "java": ">=17"
    //<KOTLIN>
   ,"fabric-language-kotlin": ">=${kotlinModVersion}"
    //</KOTLIN>
  }
}